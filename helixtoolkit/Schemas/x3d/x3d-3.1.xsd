			<xs:appinfo/>
			<xs:documentation source="http://www.web3d.org/x3d/specifications/ISO-IEC-19775-FDIS-X3dAbstractSpecification/Part01/components/envsensor.html#X3DEnvironmentalSensorNode"/>
		</xs:annotation>
