			<xs:appinfo/>
			<xs:documentation source="http://www.web3d.org/x3d/specifications/ISO-IEC-19775-Amendment1-X3DAbstractSpecification/Part01/components/enveffects.html#X3DFogOjbect"/>
		</xs:annotation>
